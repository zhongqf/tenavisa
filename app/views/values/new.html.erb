<h1>New value</h1>

<% form_for(@value) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :student_id %><br />
    <%= f.text_field :student_id %>
  </p>
  <p>
    <%= f.label :element_id %><br />
    <%= f.text_field :element_id %>
  </p>
  <p>
    <%= f.label :element_key %><br />
    <%= f.text_field :element_key %>
  </p>
  <p>
    <%= f.label :value %><br />
    <%= f.text_area :value %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', values_path %>