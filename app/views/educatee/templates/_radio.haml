- values = ["Set params to {:values => ['Value 1','Value 2']}"]
- values = eval(element.params)[:values] unless (element.params.blank? || element.params.nil? )

- values = values.invert if values.kind_of?(Hash)

%tr
  %td= form.label element.key, element.title_cn
  %td
    - values.each do |value|
      = form.radio_button(element.key, value)
      = value
      %br/
    