<body class="bp three-col">
  <div id="container">
    <div id="header">
      <% if logged_in? -%>
        Logged as <%= current_account.email %> (<%= current_account.loginable.class %>)
        <%= link_to 'Logout', logout_path %>
      <% else -%>
        <%= link_to 'Login', login_path %>
      <% end -%>
    </div>
    <div id="sidebar">Sidebar
    	<ul><li>one</li><li>two</li></ul>
    </div>
      <div id="content">
        <%= yield%>
      </div>
    <div id="footer">Footer ----------------------------</div>
  </div>
</body>
</html>